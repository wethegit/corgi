    "glob": "^7.2.0",
    "image-size": "^1.0.1",
    "md5-file": "^5.0.0",
    "next-plugin-yaml": "~1.0.1",
    "next-sitemap": "~2.5.19",
    "node-watch": "^0.7.3",
    "sass": "~1.49.7",
    "yamljs": "~0.3.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
